<?xml version="1.0"?>
<odoo>
	<record id="az_documents.expedient_form" model="ir.ui.view">
		<field name="name">az_expedients.expedient.form.inherit.az_documents.documents</field>
		<field name="model">az_expedients.expedient</field>
		<field name="inherit_id" ref="az_expedients.expedient_form"/>
		<field name="arch" type="xml">
			<xpath expr="//page[@name='interested_persons']" position="after">
				<page string="Documentos" name="documents_ids">
					<field name="documents_ids"/>
				</page>
			</xpath>
		</field>
	</record>
</odoo>