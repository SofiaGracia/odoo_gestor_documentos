<odoo>
	<record id="res_config_settings_view_form" model="ir.ui.view">
		<field name="name">res.config.settings.view.form.inherit.az_sedipualba.sedipualba</field>
		<field name="model">res.config.settings</field>
		<field name="priority" eval="15"/>
		<field name="inherit_id" ref="base.res_config_settings_view_form"/>
		<field name="arch" type="xml">
			<xpath expr="//div[hasclass('settings')]" position="inside">
				<div class="app_settings_block" data-string="AZ Sedipualba" string="AZ Sedipualba" data-key="az_sedipualba">
					<h2>Sedipualba</h2>
					<div class="row mt16 o_settings_container" id="az_sedipualba_sedipualba">
						<div class="col-12 col-lg-6 o_setting_box">
							<div class="o_setting_left_pane"></div>
							<div class="o_setting_right_pane">
								<label for="username"/><br/>
								<field name="username"/>
							</div>
						</div>
						<div class="col-12 col-lg-6 o_setting_box">
							<div class="o_setting_left_pane"></div>
							<div class="o_setting_right_pane">
								<label for="password"/><br/>
								<field name="password" password="True"/>
							</div>
						</div>
						<div class="col-12 col-lg-6 o_setting_box">
							<div class="o_setting_left_pane"></div>
							<div class="o_setting_right_pane">
								<label for="entity"/><br/>
								<field name="entity"/>
							</div>
						</div>
						<div class="col-12 col-lg-6 o_setting_box">
							<div class="o_setting_left_pane">
								<field name="mode"/>
							</div>
							<div class="o_setting_right_pane">
								<label for="mode"/>
							</div>
						</div>
					</div>
				</div>
			</xpath>
		</field>
	</record>
</odoo>