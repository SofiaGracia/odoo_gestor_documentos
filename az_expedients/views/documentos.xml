<odoo>
    <data>
        <record model="ir.ui.view" id="az_expedients_documentos_tree">
            <field name="name">az_expedients_documentos_tree</field>
            <field name="model">az_expedients.documentos</field>
            <field name="inherit_id" ref="documentos.pau_documentos_documentos_tree" />
            <field name="arch" type="xml">
                <xpath expr="//field [@name='codigo']" position="after">
                </xpath>
            </field>
        </record>

        <record id="az_expedients_documentos_form" model="ir.ui.view">
            <field name="name">az_expedocumentos.form</field>
            <field name="model">az_expedients.documentos</field>
            <field name="inherit_id" ref="documentos.pau_documentos_documentos_form" />
            <field name="arch" type="xml">
                <xpath expr="//field [@name='file_url']" position="after">
                </xpath>
            </field>
        </record>

        <record id="az_expedients.documentos_action_window" model="ir.actions.act_window">
            <field name="name">Documentos</field>
            <field name="res_model">az_expedients.documentos</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
                                        (0, 0, {'view_mode': 'tree', 'view_id': ref('az_expedients_documentos_tree')}),
                                        (0, 0, {'view_mode': 'form', 'view_id': ref('az_expedients_documentos_form')})]"/>
        </record>

    </data>
</odoo>